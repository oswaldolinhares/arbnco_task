<table class="my-5">
    <% if notice.present? %>
      <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
    <% end %>
  <thead>
    <tr>
      <th>Filename</th>
      <th>User</th>
      <th>Structure</th>
      <th>Created At</th>
    </tr>
  </thead>    
    <%= render @imports %>
</table>