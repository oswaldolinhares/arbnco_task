<table class="my-5">
    <% if notice.present? %>
      <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
    <% end %>
  <thead>
    <tr>
      <th>Import Id</th>
      <th>Object Name</th>
      <th>Object Type</th>
      <th>Properties</th>
    </tr>
  </thead>    
  <tbody>
    <% @object_items.each do |object_item| %>
      <tr>
        <td><%= object_item.import.id %></td>
        <td><%= object_item.name %></td>
        <td><%= object_item.object_type.name %></td>
        <td><%= JSON.parse(object_item.properties).to_s.truncate(50, omission: '') %>...</td>
        <td colspan="4">
          <%= link_to 'Show', object_item_path(object_item, import_id: @import.id), class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %></td>
        </td> 
      </tr>
    <% end %>
  </tbody>
</table>
